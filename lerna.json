{
  "lerna": "3.4.2",
  "packages": [
    "packages/*"
  ],
  "version": "independent",
  "useWorkspaces": true,
  "npmClient": "npm",
  "command": {
    "publish": {
      "npmClient": "npm",
      "verifyAccess": false,
      "registry": "https://registry.npmjs.org"
    }
  },
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.md"
  ]
}
